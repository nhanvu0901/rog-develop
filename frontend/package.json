{
  "name": "document-processing-frontend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.13.1",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "axios": "^1.6.7",
    "framer-motion": "^10.18.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-icons": "^4.12.0",
    "react-router-dom": "^6.22.1",
    "react-scripts": "5.0.1",
    "react-use-websocket": "^4.13.0"
  },
  "scripts": {
    "start": "WATCHPACK_POLLING=true react-scripts start",
    "build": "GENERATE_SOURCEMAP=true INLINE_RUNTIME_CHUNK=false react-scripts build && mkdir -p ../backend/www && rm -rf ../backend/www/* && cp -r build/* ../backend/www/",
    "build:dev": "cross-env NODE_ENV=development GENERATE_SOURCEMAP=true react-scripts build && mkdir -p ../backend/www && rm -rf ../backend/www/* && cp -r build/* ../backend/www/",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "cross-env": "^7.0.3"
  }
}
